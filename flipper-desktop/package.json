{
  "$schema": "https://fbflipper.com/schemas/plugin-package/v2.json",
  "name": "flipper-plugin-rn-perf-monitor",
  "id": "rn-perf-monitor",
  "version": "0.3.0",
  "main": "dist/bundle.js",
  "flipperBundlerEntry": "src/index.tsx",
  "license": "MIT",
  "keywords": [
    "flipper-plugin"
  ],
  "icon": "apps",
  "title": "RN Perf monitor",
  "description": "Alpha and experimental perf monitor for RN, like Lighthouse",
  "homepage": "https://github.com/bamlab/react-native-performance",
  "repository": {
    "type": "git",
    "url": "https://github.com/bamlab/react-native-performance"
  },
  "scripts": {
    "lint": "flipper-pkg lint",
    "prepack": "flipper-pkg lint && flipper-pkg bundle",
    "build": "flipper-pkg bundle",
    "watch": "flipper-pkg bundle --watch",
    "test": "yarn lint && prettier 'src/**/*.{ts,tsx}' --check && tsc && jest",
    "postinstall": "patch-package"
  },
  "peerDependencies": {
    "antd": "latest",
    "flipper-plugin": "*"
  },
  "devDependencies": {
    "@emotion/styled": "^11.3.0",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.0.2",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-test-renderer": "^17.0.1",
    "antd": "^4.18.5",
    "flipper-pkg": "^0.129.0",
    "flipper-plugin": "^0.129.0",
    "jest": "^27.3.1",
    "jest-environment-jsdom": "^27.3.1",
    "jest-mock-console": "^1.2.3",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "apexcharts": "^3.33.0",
    "react-apexcharts": "^1.3.9"
  }
}
